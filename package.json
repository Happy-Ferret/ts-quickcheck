{
  "name": "ts-quickcheck",
  "license": "MIT",
  "version": "0.1.0",
  "author": "Dan Ros√©n",
  "description": "Property based testing for TypeScript",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danr/ts-quickcheck.git"
  },
  "directories": {
    "test": "test"
  },
  "bugs": {
    "url": "https://github.com/danr/ts-quickcheck/issues"
  },
  "homepage": "https://github.com/danr/ts-quickcheck#readme",
  "keywords": [
    "quickcheck",
    "property",
    "verification",
    "testing",
    "tdd"
  ],
  "dependencies": {
    "json-stringify-pretty-compact": "^1.1.0"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "nyc": "^11.4.1",
    "parcel-bundler": "^1.3.1",
    "parcel-plugin-typescript": "^0.5.0-next.0",
    "prettier": "^1.9.2",
    "typescript": "^2.6.1"
  },
  "scripts": {
    "build": "parcel build src/main.ts",
    "test:watch": "tsc --watch & ava --watch dist/test/",
    "test": "tsc && ava dist/test/*js",
    "coverage": "tsc && nyc ava dist/test/*js",
    "prettier": "prettier --list-different --write \"{src,test}/**/*.ts\""
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "jsxBracketSameLine": true,
    "bracketSpacing": false
  }
}
